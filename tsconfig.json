{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    // "noEmitOnError": true,
    "skipLibCheck": true
  },
  "exclude":[
    "./node_modules"
  ]
}